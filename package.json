{
  "name": "phaser-electron-game",
  "version": "0.1.2",
  "main": "electron/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --publish always"
  },
  "build": {
    "appId": "com.wildan.phaserelectron",
    "productName": "Phaser Electron Game",
    "win": {
      "target": "nsis",
      "sign": false,
      "verifyUpdateCodeSignature": false
    },
    "publish": [
      {
        "provider": "github",
        "owner": "WildanHanifz",
        "repo": "phaser-electron-game"
      }
    ]
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "phaser": "^3.80.0"
  }
}
